{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Restaurant Response",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "address",
    "cuisine",
    "capacity",
    "period"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "cuisine": {
      "type": "string"
    },
    "capacity": {
      "type": "integer"
    },
    "period": {
      "type": "string"
    }
  }
}
