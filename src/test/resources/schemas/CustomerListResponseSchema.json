{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Customer List Response",
  "type": "array",
  "additionalProperties": false,
  "items": {
    "type": "object",
    "required": [
      "id",
      "name",
      "contact"
    ],
    "properties": {
      "id": {
        "type": "integer"
      },
      "name": {
        "type": "string"
      },
      "contact": {
        "type": "string"
      }
    }
  }
}